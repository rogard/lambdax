\documentclass{article}
\usepackage[T1]{fontenc}
\usepackage{lambda-etcetera}
\begin{document}
\ExplSyntaxOn
\START
\NewDocumentCommand
\Foo{mm}{[#1](#2)}
\START
\box_new:N\l_tmp_box
\hbox_set:Nn \l_tmp_box{\LambdaExpression[mO{w}]{\Foo{#1}{#2}}{x}[y]}
\box_show:N \l_tmp_box
\box_use:N \l_tmp_box
\hbox_set:Nn \l_tmp_box{\LambdaExpression[mO{w}]{\Foo{#1}{#2}}{x}}
\box_use:N \l_tmp_box
\box_show:N \l_tmp_box
%\END
\ExplSyntaxOff
\end{document}